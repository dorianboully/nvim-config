     {
  "inline_math": {
    "prefix": "mm",
    "body": "$${1}$$0",
    "description": "Inline math mode"
  },
  "display_math": {
    "prefix": "dm",
    "body": [
      "$",
      "\t$0", 
      "$"
    ],
    "description": "Display math mode"
  },
  "set": {
    "prefix": "set",
    "body": "{ ${1} mid(|) ${2} }$0",
    "description": "Mathematical set with |"
  },
  "set;": {
    "prefix": "set;",
    "body": "{ ${1} thick;thick ${2} }$0",
    "description": "Mathematical set with ;"
  },
  "binomial": {
    "prefix": "binom",
    "body": "binom(${1:n}, ${2:k})$0",
    "description": "Binomial coefficient"
  },
  "summation": {
    "prefix": "sm",
    "body": "sum",
    "description": "Summation"
  },
  "product": {
    "prefix": "pr",
    "body": "product",
    "description": "Product"
  },
  "coproduct": {
    "prefix": "cpr",
    "body": "product.co",
    "description": "Coproduct"
  },
  "direct_sum": {
    "prefix": "bop",
    "body": "op(plus.circle)",
    "description": "Direct sum"
  },
  "big_intersection": {
    "prefix": "bcap",
    "body": "sect.big",
    "description": "Big intersection"
  },
  "big_union": {
    "prefix": "bcup",
